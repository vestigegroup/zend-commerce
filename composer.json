{
    "name": "ltouro/zend-commerce",
    "description": "High leveled abstract E-Commerce module for Zend Framework 2.2",
    "minimum-stability": "dev",
    "license": "BSD-3-Clause",
    "keywords": [
        "e-commerce",
        "zf2",
	"doctrine"
    ],
    "homepage": "http://zend-commerce.com/",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "dev-master",
        "doctrine/doctrine-module": "dev-master",
        "doctrine/orm": "dev-master as 2.6",
        "doctrine/doctrine-orm-module": "dev-master",
        "bjyoungblood/bjy-profiler": "dev-master",
        "bjyoungblood/bjy-authorize":"1.4.*",
        "zf-commons/zfc-user-doctrine-orm":"0.1.*",
        "zf-commons/zfc-admin":"0.1.*",
        "dropbox/dropbox-sdk":"dev-master",
        "socalnick/scn-social-auth": "1.12.0",
        "socalnick/scn-social-auth-doctrine-orm": "dev-master",
        "league/flysystem": "0.2.*",
        "ltouro/moip-php": "dev-master",
        "paypal/rest-api-sdk-php": "dev-master",
        "phpunit/phpunit":"3.7.*",
        "mathiasverraes/money": "dev-master"

    },
    "require-dev":{
        "zendframework/zend-developer-tools": "dev-master",
        "zendframework/zftool": "dev-master",
        "oodle/krumo": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "LtCommons": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}

