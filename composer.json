        "socalnick/scn-social-auth": "1.12.0",
        "socalnick/scn-social-auth-doctrine-orm": "1.3.0",
        "league/flysystem": "0.2.*",
        "ltouro/moip-php": "dev-master",
        "paypal/rest-api-sdk-php": "0.7.*",
        "phpunit/phpunit":"3.7.*",
        "mathiasverraes/money": "1.2.0"
    },
    "require-dev":{
